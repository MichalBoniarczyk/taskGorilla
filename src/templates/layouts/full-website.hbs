<!doctype html>
<!--[if IE 8]><html lang="en" class="ie8"><![endif]-->
<!--[if IE 9]><html lang="en" class="ie9"><![endif]-->
<!--[if gt IE 9]><!--><html lang="en"><!--<![endif]-->
<head>
    {{> head }}
</head>
<body>
    <div class="page-wrap">
        {{> header }}
        <div id="content" class="content">
            <div class="container-fluid">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 details-block">
                    {{> details }}
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 site-content">
                    <div class="left-navigation col-lg-3 col-md-2 col-sm-12 col-xs-12" role="complementary">
                        {{> left }}
                    </div>
                    <div class="main col-lg-9 col-md-10 col-sm-12 col-xs-12" role="complementary">
                        {{> right }}
                    </div>
                </div>
            </div>
        </div>
        {{> footer }}
    </div>
    {{> scripts}}
</body>
</html>